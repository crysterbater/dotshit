{
   "background": {
      "scripts": [ "jquery.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "img/ico_19.png",
      "default_popup": "popup.html"
   },
   "content_security_policy": "script-src 'self' https://*.google.com https://*.google-analytics.com https://*.doubleclick.net; object-src 'self'",
   "description": "Flash Player + save your favorite flash and run this later in player.",
   "icons": {
      "128": "img/ico_128.png",
      "16": "img/ico_16.png",
      "48": "img/ico_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Flash Player +",
   "options_page": "options.html",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "contextMenus", "notifications", "unlimitedStorage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2"
}
